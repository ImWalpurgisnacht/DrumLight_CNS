<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#2B2B2B"
    tools:ignore="MissingDefaultResource">

    <TextView
        android:id="@+id/text"
        android:layout_width="311dp"
        android:layout_height="wrap_contents"
        android:text = "위치를 맞춰주세요!"
        android:textSize="36dp"
        android:textColor="#F6E7B2"
        android:textStyle="bold"
        android:layout_marginStart="245dp"
        android:layout_marginTop = "34dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/drum1"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="177dp"
        android:layout_marginTop = "100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/drum2"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="304dp"
        android:layout_marginTop = "141dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/drum3"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="416dp"
        android:layout_marginTop = "141dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/drum4"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="528dp"
        android:layout_marginTop = "100dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/drum5"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="249dp"
        android:layout_marginTop = "241dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/drum6"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="356dp"
        android:layout_marginTop = "221dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <ImageView
        android:id="@+id/drum"
        android:layout_width="95dp"
        android:layout_height="95dp"
        android:src="@drawable/drum"
        android:layout_marginStart="454dp"
        android:layout_marginTop = "242dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        />

    <Button
        android:id="@+id/next"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="690dp"
        android:layout_marginTop = "295dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@drawable/level"
        android:text = "→"
        android:textSize = "55dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/how"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:src = "@drawable/how"
        android:layout_marginStart="742dp"
        android:layout_marginTop = "20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:background="@android:color/transparent"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>